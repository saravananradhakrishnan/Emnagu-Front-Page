
<!DOCTYPE html>
<html ng-app="Emangu">
<head>
    <title>DeviceTest</title>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => false %>
     <%= stylesheet_link_tag    "application", media: "all" %>
    <%= csrf_meta_tags %>
</head>
<body>
<% if notice %>
    <p class="alert alert-success"><%= notice %></p>
<% end %>
<% if alert %>
    <p class="alert alert-danger"><%= alert %></p>
<% end %>
 <!-- Header -->
 <div ng-include src="'template/partials/_header.html'"></div>

 <!-- Dynamic Body -->
    <div  ng-view>
        <%= yield %>
    </div>

    <!-- Footer -->

        <div ng-include src="'template/partials/_footer.html'" ></div>

    </body>
</html>
